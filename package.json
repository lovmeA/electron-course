{
  "name": "course",
  "version": "1.0.0",
  "description": "自考校考答题答案 - 晴天雨露",
  "main": "main.js",
  "scripts": {
    "build:dist": "electron-builder",
    "dev": "nodemon --exec electron . --watch . --ext .js,.html,.vue,.css",
    "start": "electron ."
  },
  "build": {
    "productName": "自考校考答题答案",
    "directories": {
      "output": "dist"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "win": {
      "target": "nsis",
      "icon": "Course.ico"
    },
    "extraResources": [
      {
        "from": "Course.ico",
        "to": "../Course.ico"
      },
      {
        "from": "db.sqlite3",
        "to": "../db.sqlite3"
      }
    ]
  },
  "author": "晴天雨露",
  "license": "ISC",
  "devDependencies": {
    "electron": "19.0.0",
    "electron-builder": "^23.6.0",
    "node-gyp": "^9.3.1",
    "nodemon": "^2.0.20"
  },
  "dependencies": {
    "asar": "^3.2.0",
    "electron-win-state": "^1.1.22",
    "knex": "^2.3.0",
    "sqlite3": "^5.1.4"
  }
}
